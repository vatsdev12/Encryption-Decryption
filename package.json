{
  "name": "@manishtickets/encryption-decryption",
  "version": "1.1.0",
  "description": "A TypeScript library for secure encryption and decryption using Google Cloud KMS and Secret Manager",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/vatsdev12/Encryption-Decryption/tree/feature/package-creation",
  "files": [
    "dist",
    "README.md"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "encryption",
    "decryption",
    "kms",
    "secret-manager",
    "google-cloud",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/kms": "^5.0.1",
    "@google-cloud/secret-manager": "^6.0.1",
    "sequelize": "^6.37.7",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "sequelize": "^6.37.7"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vatsdev12/Encryption-Decryption.git"
  },
  "bugs": {
    "url": "https://github.com/vatsdev12/Encryption-Decryption/issues"
  }
}
